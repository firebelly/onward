{
  "require": {
    "aelvan/imager": "v2.4.0",
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/cms": "3.4.22.1",
    "craftcms/contact-form": "2.2.7",
    "craftcms/contact-form-honeypot": "^1.0",
    "craftcms/redactor": "2.6.1",
    "drewm/mailchimp-api": "^2.5",
    "hybridinteractive/craft-contact-form-extensions": "1.2.1",
    "mmikkel/cp-field-inspect": "1.1.3",
    "mmikkel/retcon": "2.1.0",
    "pelago/emogrifier": "^2.0",
    "spicyweb/craft-embedded-assets": "2.2.6",
    "verbb/expanded-singles": "1.1.2",
    "verbb/super-table": "2.5.0",
    "vlucas/phpdotenv": "*",
    "wbrowar/adminbar": "3.1.8.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
