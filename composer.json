{
  "require": {
    "craftcms/cms": "3.2.10",
    "verbb/super-table": "2.2.1",
    "vlucas/phpdotenv": "^3.4.0",
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/redactor": "2.3.3.2",
    "aelvan/imager": "v2.1.10",
    "pelago/emogrifier": "^2.0",
    "verbb/expanded-singles": "1.0.7",
    "mmikkel/cp-field-inspect": "1.0.6",
    "spicyweb/craft-embedded-assets": "2.0.11",
    "barrelstrength/sprout-active": "2.1.0",
    "wbrowar/adminbar": "3.1.8.1",
    "mmikkel/retcon": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
